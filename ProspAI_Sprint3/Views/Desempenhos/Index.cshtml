@model IEnumerable<ProspAI_Sprint3.Models.Desempenho>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Mes_desem)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Reclamacoes_resp)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Reclamacoes_solu)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Funcionario)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Mes_desem)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Reclamacoes_resp)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Reclamacoes_solu)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Funcionario.Email_fun)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id_desem">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id_desem">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id_desem">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
